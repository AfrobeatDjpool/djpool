<%= form_with(model: rent, local: true, multipart: true) do |f| %>
<div class="row">
  <div class="container">
    <div id="loginbox"  class="mainbox col-md-6 col-md-offset-3 col-sm-7 col-sm-offset-2 allSides box-outernew">
      <div class="panel panel-info " >
        <div class="panel-heading">
          <div class="panel-title font-bold" >Create Rent</div>
          <div class="panel-head"></div>
        </div>
        <div  class="panel-body panel-bodylogin-signin" >
          <div style="display:none" id="login-alert" class="alert alert-danger col-sm-12"></div>
          <div class="form-inputs">
              <% if rent.errors.any? %>
                  <div id="error_explanation">
                    <h2><%= pluralize(rent.errors.count, "error") %> prohibited this rent from being saved:</h2>

                    <ul>
                    <% rent.errors.full_messages.each do |message| %>
                      <li><%= message %></li>
                    <% end %>
                    </ul>
                  </div>
                <% end %>
            <br><br> 
            <div class="form-group">
              <label for="inputEmail3" class="col-sm-5 control-label">Upload FIle</label>
              <div class="col-sm-7">
                <input type="file" class="form-control " id="inputEmail3" name="file" placeholder="Select CSV File">
              </div>
            </div>
            <br><br>
     
            <div class="form-group ">
              <div class="col-sm-7 ">
                <br>
                <%= f.submit "SAVE", :class=>"btn btn-primary "%>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<% end %>
<br>
<br>

