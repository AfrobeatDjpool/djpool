<%= link_to 'New Rent', new_rent_path, class: "btn btn-primary" %>

<table class="table">
  <thead>
    <tr>
      <th>Patente</th>
      <th>Dv_patente</th>
      <th>Brand</th>
      <th>Model</th>
      <th>Tipo</th>
      <th>Date_ult_trans</th>
      <th>Color</th>
      <th>Rest_color</th>
      <th>Ano_fab</th>
      <th>Chasis</th>
      <th>Numero_motor</th>      
      <th>Rut</th>
      <th>Dv</th>
      <th>name</th>
      <th>Fines</th>
    
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rents.each do |rent| %>
      <tr>
        <td><%= rent.patente %></td>
        <td><%= rent.dv_patente %></td>
        <td><%= rent.brand %></td>
        <td><%= rent.model %></td>
        <td><%= rent.tipo %></td>
        <td><%= rent.date_ult_trans %></td>
        <td><%= rent.color %></td>
        <td><%= rent.rest_color %></td>
        <td><%= rent.ano_fab %></td>
        <td><%= rent.chasis %></td>
        <td><%= rent.numero_motor %></td>
        <td><%= rent.numero_motor %></td>
        <td><%= rent.rut %></td>
        <td><%= rent.dv %></td>
        <td><%= rent.first_name %></td>
        <td><%= rent.fines %></td>
        
        <td><%= link_to 'Show', rent %></td>
        <td><%= link_to 'Edit', edit_rent_path(rent) %></td>
        <td><%= link_to 'Destroy', rent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class="digg_pagination">
    <div class="page_info">
      <%= page_entries_info @rents %>
    </div>
    <%= will_paginate @rents, :container => false %>
  </div>

<br>

